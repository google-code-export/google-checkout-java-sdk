<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <display-name>Google Storefront</display-name>

  <servlet>
    <servlet-name>ProjectPropertiesReader</servlet-name>
    <servlet-class>
      com.google.checkout.samples.samplestore.server.ProjectPropertiesReaderImpl
    </servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ProjectPropertiesReader</servlet-name>
    <url-pattern>/propertiesReader</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>GridStore.html</welcome-file>
  </welcome-file-list>
</web-app>