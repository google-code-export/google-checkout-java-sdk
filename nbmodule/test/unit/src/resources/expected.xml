<checkout-config>
    <merchant-info>
        <merchant-id></merchant-id>
        <merchant-key></merchant-key>
        <env>Sandbox</env>
        <currency-code>USD</currency-code>
        <sandbox-root>https://sandbox.google.com/checkout/cws/v2/Merchant</sandbox-root>
        <production-root>https://checkout.google.com/cws/v2/Merchant</production-root>
        <checkout-suffix>checkout</checkout-suffix>
        <merchant-checkout-suffix>merchantCheckout</merchant-checkout-suffix>
        <request-suffix>request</request-suffix>
    </merchant-info>
    <notification-handlers>
        <notification-handler>
            <message-type>authorization-amount-notification</message-type>
            <handler-class>com.google.checkout.sdk.AuthorizationAmountNotificationHandler</handler-class>
        </notification-handler>
        <notification-handler>
            <message-type>charge-amount-notification</message-type>
            <handler-class>com.google.checkout.sdk.ChargeAmountNotificationHandler</handler-class>
        </notification-handler>
        <notification-handler>
            <message-type>order-state-change-notification</message-type>
            <handler-class>com.google.checkout.sdk.OrderStateChangeNotificationHandler</handler-class>
        </notification-handler>
        <notification-handler>
            <message-type>risk-information-notification</message-type>
            <handler-class>com.google.checkout.sdk.RiskInformationNotificationHandler</handler-class>
        </notification-handler>
        <notification-handler>
            <message-type>refund-amount-notification</message-type>
            <handler-class>com.google.checkout.sdk.RefundAmountNotificationHandler</handler-class>
        </notification-handler>
        <notification-handler>
            <message-type>chargeback-amount-notification</message-type>
            <handler-class>com.google.checkout.sdk.ChargebackAmountNotificationHandler</handler-class>
        </notification-handler>
        <notification-handler>
            <message-type>new-order-notification</message-type>
            <handler-class>com.google.checkout.sdk.NewOrderNotificationHandler</handler-class>
        </notification-handler>
    </notification-handlers>
    <callback-handlers>
        <callback-handler>
            <message-type>merchant-calculation-callback</message-type>
            <handler-class>com.google.checkout.sdk.MerchantCalculationCallbackHandler</handler-class>
        </callback-handler>
    </callback-handlers>
</checkout-config>